<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.keyss.miwok">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_miwok"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activityNumbers"
            android:label="Numbers"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".activityColors"
            android:label="Colors"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".activityFamilyMembers"
            android:label="Family Members"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".activityPhrases"
            android:label="Phrases"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".about_app"
            android:parentActivityName=".MainActivity"
         />
        <activity
            android:name=".sign_in"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".knowledge_page"
            android:parentActivityName=".MainActivity" />

    <activity
        android:name=".Extra"
        android:parentActivityName=".MainActivity" />
</application>

</manifest>